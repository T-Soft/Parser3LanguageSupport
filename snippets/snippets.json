{
    "DocumentingSummary": {
        "prefix": "doc",
        "body": [
			"### <summary>",
			"### ${1:summaryText}",
			"### </summary>"			
        ],
        "description": "Insert dcumenting comment"
	},
	"DocumentingParameter": {
        "prefix": "docparam",
        "body": [
			"### <param name=\"${1:paramName}\">${2:paramDescription}</param>"
        ],
        "description": "Insert parameter dcumenting comment"
	},
	"DocumentingRemarks": {
        "prefix": "docremarks",
        "body": [
			"### <remarks>",
			"### ${1:remarksText}",
			"### </remarks>"
        ],
        "description": "Insert dcumenting comment"
	},
	"WrapInRegion":{
		"prefix": "wrapreg",
        "body": [
			"#region ${1:regionName}",
			"$TM_SELECTED_TEXT",
			"#endregion"
        ],
        "description": "Wrap selected text in folding region"
	},
	"Region":{
		"prefix": "region",
        "body": [
			"#region ${1:regionName}"
        ],
        "description": "Insert folding region start"
	},
	"Endregion":{
		"prefix": "endregion",
        "body": [
			"#endregion"
        ],
        "description": "Insert folding region end"
	}
}